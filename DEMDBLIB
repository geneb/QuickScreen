#DEMDBLIB. - Make Description File for DEMDBLIB.EXE.
#
DEMDBLIB.OBJ:  DEMDBLIB.BAS DefCnf.bi FieldInf.bi FormEdit.bi SetCnf.bi
   BC DEMDBLIB.BAS /s/o;

VERTMENU.OBJ:  VERTMENU.BAS DefCnf.BI
   BC VERTMENU.BAS /s/o;

NOCALC.OBJ:  NOCALC.BAS FieldInf.bi
   BC NOCALC.BAS /s/o;

SCRNDISP.OBJ:  SCRNDISP.BAS
   BC SCRNDISP.BAS /s/o;

FORMEDIT.OBJ:  FORMEDIT.BAS DefCnf.bi FieldInf.bi FormEdit.bi QEditype.bi
   BC FORMEDIT.BAS /s/o;

DBLIBMOD.OBJ:  DBLIBMOD.BAS FieldInf.bi
   BC DBLIBMOD.BAS /s/o;

QEDITS.OBJ:  QEDITS.BAS QEditype.BI
   BC QEDITS.BAS /s/o;

FORMFILE.OBJ:  FORMFILE.BAS FieldInf.bi
   BC FORMFILE.BAS /s/o;

SCRNFILE.OBJ:  SCRNFILE.BAS
   BC SCRNFILE.BAS /s/o;

DEMDBLIB.EXE:  DEMDBLIB.OBJ VERTMENU.OBJ NOCALC.OBJ SCRNDISP.OBJ FORMEDIT.OBJ\
               DBLIBMOD.OBJ QEDITS.OBJ FORMFILE.OBJ SCRNFILE.OBJ FORMS.LIB\
               DBLIB.LIB
   LINK @DEMDBLIB.RSP
   DIR DEMDBLIB.EXE
