# DEMOCUST. - Make Description File for DEMOCUST.EXE.
#
DEMOCUST.OBJ:  DEMOCUST.BAS DefCnf.bi FieldInf.bi FormEdit.bi SetCnf.bi
   BC DEMOCUST.BAS /s/o;

FORMEDIT.OBJ:  FORMEDIT.BAS DefCnf.bi FieldInf.bi FormEdit.bi QEditype.bi
   BC FORMEDIT.BAS /s/o;

NOMULT.OBJ:  NOMULT.BAS DefCnf.BI
   BC NOMULT.BAS /s/o;

NOCALC.OBJ:  NOCALC.BAS FieldInf.bi
   BC NOCALC.BAS /s/o;

SCRNDISP.OBJ:  SCRNDISP.BAS
   BC SCRNDISP.BAS /s/o;

FORMLIB.OBJ:  FORMLIB.BAS FieldInf.bi
   BC FORMLIB.BAS /s/o;

SCRNLIB.OBJ:  SCRNLIB.BAS
   BC SCRNLIB.BAS /s/o;

RANDOMIO.OBJ:  RANDOMIO.BAS FieldInf.bi
   BC RANDOMIO.BAS /s/o;

QEDITS.OBJ:  QEDITS.BAS QEditype.BI
   BC QEDITS.BAS /s/o;

DEMOCUST.EXE:  DEMOCUST.OBJ FORMEDIT.OBJ NOMULT.OBJ NOCALC.OBJ SCRNDISP.OBJ\
               FORMLIB.OBJ SCRNLIB.OBJ RANDOMIO.OBJ QEDITS.OBJ PRO.LIB
   LINK @DEMOCUST.RSP
   DIR DEMOCUST.EXE
