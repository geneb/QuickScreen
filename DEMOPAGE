# DEMOPAGE. - Make Description File for DEMOPAGE.EXE.
#
DEMOPAGE.OBJ:  DEMOPAGE.BAS DefCnf.bi FieldInf.bi FormEdit.bi SetCnf.bi
   BC DEMOPAGE.BAS /s/o;

EVALUATE.OBJ:  EVALUATE.BAS
   BC EVALUATE.BAS /s/o;

QSCALC.OBJ:  QSCALC.BAS FieldInf.bi
   BC QSCALC.BAS /s/o;

SCRNDISP.OBJ:  SCRNDISP.BAS
   BC SCRNDISP.BAS /s/o;

FORMEDIT.OBJ:  FORMEDIT.BAS DefCnf.bi FieldInf.bi FormEdit.bi QEditype.bi
   BC FORMEDIT.BAS /s/o;

VERTMENU.OBJ:  VERTMENU.BAS DefCnf.BI
   BC VERTMENU.BAS /s/o;

QEDITS.OBJ:  QEDITS.BAS QEditype.BI
   BC QEDITS.BAS /s/o;

FORMLIB.OBJ:  FORMLIB.BAS FieldInf.bi
   BC FORMLIB.BAS /s/o;

SCRNLIB.OBJ:  SCRNLIB.BAS
   BC SCRNLIB.BAS /s/o;

DEMOPAGE.EXE:  DEMOPAGE.OBJ EVALUATE.OBJ QSCALC.OBJ SCRNDISP.OBJ FORMEDIT.OBJ\
               VERTMENU.OBJ QEDITS.OBJ FORMLIB.OBJ SCRNLIB.OBJ FORMS.LIB
   LINK @DEMOPAGE.RSP
   DIR DEMOPAGE.EXE
