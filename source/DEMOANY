# DEMOANY. - Make Description File for DEMOANY.EXE.
#
DEMOANY.OBJ:  DEMOANY.BAS FieldInf.bi FormEdit.bi DefCnf.bi SetCnf.bi
   BC DEMOANY.BAS /s/o;

QSCALC.OBJ:  QSCALC.BAS FieldInf.bi
   BC QSCALC.BAS /s/o;

FORMEDIT.OBJ:  FORMEDIT.BAS DefCnf.bi FieldInf.bi FormEdit.bi QEditype.bi
   BC FORMEDIT.BAS /s/o;

SCRNDISP.OBJ:  SCRNDISP.BAS
   BC SCRNDISP.BAS /s/o;

EVALUATE.OBJ:  EVALUATE.BAS
   BC EVALUATE.BAS /s/o;

FORMFILE.OBJ:  FORMFILE.BAS FieldInf.bi
   BC FORMFILE.BAS /s/o;

SCRNFILE.OBJ:  SCRNFILE.BAS
   BC SCRNFILE.BAS /s/o;

VERTMENU.OBJ:  VERTMENU.BAS DefCnf.BI
   BC VERTMENU.BAS /s/o;

QEDITS.OBJ:  QEDITS.BAS QEditype.BI
   BC QEDITS.BAS /s/o;

DEMOANY.EXE:  DEMOANY.OBJ QSCALC.OBJ FORMEDIT.OBJ SCRNDISP.OBJ EVALUATE.OBJ\
              FORMFILE.OBJ SCRNFILE.OBJ VERTMENU.OBJ QEDITS.OBJ FORMS.LIB
   LINK @DEMOANY.RSP
   DIR DEMOANY.EXE
