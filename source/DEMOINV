   BC QEDITS.BAS /s/o;

EVALUATE.OBJ:  EVALUATE.BAS
   BC EVALUATE.BAS /s/o;

QSCALC.OBJ:  QSCALC.BAS FieldInf.bi
   BC QSCALC.BAS /s/o;

SCRNDISP.OBJ:  SCRNDISP.BAS
   BC SCRNDISP.BAS /s/o;

FORMEDIT.OBJ:  FORMEDIT.BAS DefCnf.bi FieldInf.bi FormEdit.bi QEditype.bi
   BC FORMEDIT.BAS /s/o;

VERTMENU.OBJ:  VERTMENU.BAS DefCnf.BI
   BC VERTMENU.BAS /s/o;

SCRNFILE.OBJ:  SCRNFILE.BAS
   BC SCRNFILE.BAS /s/o;

FORMFILE.OBJ:  FORMFILE.BAS FieldInf.bi
   BC FORMFILE.BAS /s/o;

DEMOINV.EXE:  DEMOINV.OBJ QEDITS.OBJ EVALUATE.OBJ QSCALC.OBJ SCRNDISP.OBJ\
              FORMEDIT.OBJ VERTMENU.OBJ SCRNFILE.OBJ FORMFILE.OBJ FORMS.LIB
   LINK @DEMOINV.RSP
   DIR DEMOINV.EXE
